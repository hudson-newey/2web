{
  "name": "@two-web/kit",
  "version": "0.0.1-rc.3",
  "description": "A collection of packages to enhance your 2web application",
  "author": "hudson-newey",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hudson-newey/2web.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite ./dev/",
    "build": "vite build",
    "test": "vitest",
    "format": "prettier --write",
    "lint": "oxlint"
  },
  "files": [
    "dist/",
    "vite-plugin/"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./animations": {
      "import": "./dist/animations.js",
      "types": "./dist/animations/index.d.ts"
    },
    "./iron": {
      "import": "./dist/iron.js",
      "types": "./dist/iron/index.d.ts"
    },
    "./keyboard": {
      "import": "./dist/keyboard.js",
      "types": "./dist/keyboard/index.d.ts"
    },
    "./named-strings": {
      "import": "./dist/named-strings.js",
      "types": "./dist/named-strings/index.d.ts"
    },
    "./pre-fetcher": {
      "import": "./dist/pre-fetcher.js",
      "types": "./dist/pre-fetcher/index.d.ts"
    },
    "./route-guards": {
      "import": "./dist/route-guards.js",
      "types": "./dist/route-guards/index.d.ts"
    },
    "./signals": {
      "import": "./dist/signals.js",
      "types": "./dist/signals/index.d.ts"
    },
    "./ssr": {
      "import": "./dist/ssr.js",
      "types": "./dist/ssr/index.d.ts"
    },
    "./typescript": {
      "import": "./dist/typescript.js",
      "types": "./dist/typescript/index.d.ts"
    },
    "./2js": {
      "import": "./dist/2js.js",
      "types": "./dist/2js/index.d.ts"
    },
    "./view-transitions": {
      "import": "./dist/view-transitions/*",
      "types": "./dist/view-transitions/index.d.ts"
    },
    "./vite-plugin": {
      "import": "./vite-plugin/index.ts",
      "types": "./vite-plugin/index.d.ts"
    }
  },
  "dependencies": {
    "express": "^5.1.0",
    "happy-dom": "17.4.7"
  },
  "devDependencies": {
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/express": "^5.0.2",
    "@types/node": "^22.14.1",
    "oxlint": "^1.28.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-static-copy": "^3.1.2",
    "vitest": "3.1.2"
  },
  "peerDependencies": {
    "express": "^5.1.0",
    "happy-dom": "17.4.7",
    "vite": "^6.3.4"
  },
  "packageManager": "pnpm@10.8.0"
}
